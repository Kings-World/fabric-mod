{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>Kings-World/.github:renovate-config"
  ],
  "packageRules": [
    {
      "enabled": false,
      "matchPackageNames": [
        "/fabric-loom/",
        "/^net.fabricmc/"
      ]
    },
    {
      "enabled": false,
      "matchPackageNames": [
        "/^net.sf.trove4j/",
        "/^org.apache.commons/",
        "/^com.neovisionaries/",
        "/^com.squareup/",
        "/^com.fasterxml.jackson.core/",
        "/^org.json/"
      ]
    }
  ]
}
