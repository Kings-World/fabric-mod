{
  "schemaVersion": 1,
  "id": "fabric-mod",
  "version": "${version}",

  "name": "Fabric Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Seren_Modz 21"
  ],
  "contact": {
    "homepage": "https://kings-world.net",
    "issues": "https://github.com/Kings-World/fabric-mod/issues",
    "sources": "https://github.com/Kings-World/fabric-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/fabric-mod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "me.seren.Mod"
      }
    ]
  },
  "mixins": [
    "fabric-mod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.8.1+kotlin.1.7.0"
  }
}
